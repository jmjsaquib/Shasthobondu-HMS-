                            Loading(true);
                            tabstrip.enable(tabstrip.tabGroup.children("li:eq(1)"), true);
                            tabstrip.enable(tabstrip.tabGroup.children("li:eq(2)"), true);
                            tabstrip.select(1);


                            var fData = new FormData();
                            fData.append("emp_firstname", $("#emp_firstname").val());
                            fData.append("emp_lastname", $("#emp_lastname").val());
                            fData.append("emp_dateofbirth", $("#emp_dateofbirth").val());
                            fData.append("emp_gender", $("#emp_gender").val());
                            fData.append("emp_blood_group", $("#emp_blood_group").val());
                            fData.append("emp_marital_status", $("#emp_marital_status").val());
                            fData.append("emp_id_type", $("#emp_id_type").val());
                            fData.append("emp_id_no", $("#emp_id_no").val());

                            $.ajax({
                                type: "POST",
                                url: "@UrlConfig.Action("Post", "Employee")",
                                data: fData,

                                contentType: false,
                                processData: false,
                                success: function (data) {
                                    Loading(false);
                                    if (data.output === "success") {

                                        NotificationMessage(data);
                                        // KendoWindowFunction(data.msg, "success", "");
                                        $("#emp_id").val(data.returnvalue);
                                        $("#emp_id_contact").val(data.returnvalue);
                                        return false;
                                    } else {
                                        NotificationMessage(data);
                                        // KendoWindowFunction(data.msg, "error", "");
                                        return false;
                                    }
                                }


                            });

                        });
                    });

                    function dateEditor(container, options) {
                        $('<input data-text-field="' + options.field + '" data-value-field="' + options.field + '" data-bind="value:' + options.field + '" data-format="' + options.format + '"/>')
                                .appendTo(container)
                                .kendoDatePicker({

