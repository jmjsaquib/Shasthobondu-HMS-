<script type="text/javascript">
    $(document).ready(function () {

        $.ajax({
            type: "GET",
            url: "@UrlConfig.Action("GetEmployeeByPayrollSalaryID", "Employee")?salary_head_permission=employee_module",
            dataType: "json",
            success: function (data) {
                Loading(false);

                $("#frmSalaryComponent").html('');
                var frmString = '';
                //frmString += '<form id="employee_salary">';
                console.log(data);
                var i = 1;
                $.each(data, function (key, frmelements) {

                    console.log(frmelements);

                    if (i <= 2) {

                        frmString += '<div class="col-md-5">';
                        frmString += frmelements.psh_title;
                        var prop_value_id = 'prop_' + frmelements.psh_id + '_id';
                        var prop_value_name = 'prop_' + frmelements.psh_id + '_name';
                        frmString += '<input style="width:60%;margin-left:18%" class="k-textbox" type="text" id="' + prop_value_id + '" name="' + prop_value_id + '_input">'
                        frmString += '</div>';
                    }

                    if (i == 2) {
                        frmString += '<div class="clearfix"></div><br /></div>';
                        i = 1;
                    }
                    i++;
                });

                //frmString += '</form>';
                $("#frmSalaryComponent").html(frmString);
            }
        });
    });
</script>


