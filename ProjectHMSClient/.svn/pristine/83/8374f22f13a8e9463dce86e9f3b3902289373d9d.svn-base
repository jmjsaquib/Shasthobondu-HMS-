<script type="text/javascript">
    $(document).ready(function () {
        $('#grid_3').hide();
        $('#grid_2').hide();
        $('#grid_4').hide();
        $('#common-Field_2').hide();
        $('#common-Field_3').hide();
        $('#common-Field_4').hide();

        $('#btnSubmitOrder_2').hide();
        $('#btnSubmitOrder_3').hide();
        $('#btnSubmitOrder_4').hide();


        $('#2_partset').change(function () {
            if (this.checked) {
                $('#grid_2').show();
                $('#btnSubmitOrder_2').show();
                $('#common-Field_2').show();
                $('#common-Field_3').hide();
                $('#common-Field_4').hide();
                $('#grid_3').hide();
                $('#grid_4').hide();
                $('#btnSubmitOrder_3').hide();
                $('#btnSubmitOrder_4').hide();

                $('#3_partset').not(this).prop('checked', false);
                $('#4_partset').not(this).prop('checked', false);

            } else {
                $('#grid_2').hide();

                $('#common-Field_2').hide();

                $('#btnSubmitOrder_2').hide();
            }
        });

        $('#3_partset').change(function () {
            if (this.checked) {
                $('#grid_3').show();
                $('#grid_2').show();
                $('#grid_4').hide();

                $('#common-Field_2').show();
                $('#common-Field_3').show();
                $('#common-Field_4').hide();

                $('#btnSubmitOrder_2').show();
                $('#btnSubmitOrder_3').show();
                $('#btnSubmitOrder_4').hide();

                $('#2_partset').not(this).prop('checked', false);
                $('#4_partset').not(this).prop('checked', false);

            } else {
                $('#grid_2').hide();
                $('#grid_3').hide();

                $('#common-Field_2').hide();
                $('#common-Field_3').hide();

                $('#btnSubmitOrder_2').hide();
                $('#btnSubmitOrder_3').hide();
            }
        });

        $('#4_partset').change(function () {
            if (this.checked) {
                $('#grid_3').show();
                $('#grid_2').show();
                $('#grid_4').show();

                $('#common-Field_2').show();
                $('#common-Field_3').show();
                $('#common-Field_4').show();

                $('#btnSubmitOrder_2').show();
                $('#btnSubmitOrder_3').show();
                $('#btnSubmitOrder_4').show();

                $('#3_partset').not(this).prop('checked', false);
                $('#2_partset').not(this).prop('checked', false);

            } else {
                $('#grid_3').hide();
                $('#grid_2').hide();
                $('#grid_4').hide();

                $('#common-Field_2').hide();
                $('#common-Field_3').hide();
                $('#common-Field_4').hide();

                $('#btnSubmitOrder_2').hide();
                $('#btnSubmitOrder_3').hide();
                $('#btnSubmitOrder_4').hide();
            }
        });
    });
</script>
