<!--Javascript and Ajax to Submit the Form-->
<script type="text/javascript">
    $(document).ready(function () {
        var item_style = '@ViewBag.item_style';
        var item_color = '@ViewBag.item_color';
        var item_PO = '@ViewBag.item_PO';
        var supplier_code = '@ViewBag.supplier_code';
        var rbo_id = '@ViewBag.rbo_id';

        console.log(item_color);
        console.log(item_style);

        Loading(true);

        $.ajax({

            type: "GET",
            url: "@UrlConfig.Action("AllOrders", "OrderEntry")?item_style=" + item_style + "&item_color=" + item_color + "&item_PO=" + item_PO + "&rbo_id=" + rbo_id + "&supplier_code=" + supplier_code,

            dataType: "json",
            success: function (data) {
                Loading(false);

                //Assign  to form fields
                $("#factory_name_result").html(data[0].supplier_name);
                //$("#PO_result").html(data[0].prop_8);
                //$("#style_no_result").html(data[0].prop_3);
                // $("#color_code").html(data[0].prop_2);
                $("#supplier_code_main").html(data[0].supplier_code);
                $("#temp_rbo_id").val(data[0].rbo_id);
                $("#temp_supplier_code").val(data[0].supplier_code);
            }

        });

        var label_references = $("#label_reference_id").kendoComboBox({
            placeholder: "Select Label Type...",
            dataTextField: "label_reference_name",
            dataValueField: "label_reference_id",
            dataSource: {
                transport: {
                    read: {
                        url: "@UrlConfig.Action("GetAllLabelTypes", "LabelTypeList")",
                        type: "GET"
                    }
                },

            }
        }).data("kendoComboBox");


    });
</script>

