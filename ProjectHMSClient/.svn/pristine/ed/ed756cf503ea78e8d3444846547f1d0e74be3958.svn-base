                     delivery_contact_person: delivery_contact_person
                 },
                 {
                     label_type_id: label_type_id_21,
                     label_reference_id: label_reference_id_21,
                     rbo_id: rbo_id,
                     supplier_id: supplierid,
                     COF: COF,
                     order_code: order_code,
                     job_id: job_id_21,
                     department: department,
                     season: season,
                     exclusive_trims: exclusive_trims,
                     fiber_1: fiber_1,
                     fiber_per_1: fiber_per_1,
                     kvi_dialog: kvi_dialog,
                     customer_address: customer_address,
                     delivery_address: delivery_address,
                     contact_person_customer: contact_person_customer,
                     contact_person_delivery: contact_person_delivery,
                     customer_contact_person: customer_contact_person,
                     delivery_contact_person: delivery_contact_person
                 },
                 {
                     label_type_id: label_type_id_22,
                     label_reference_id: label_reference_id_22,
                     rbo_id: rbo_id,
                     supplier_id: supplierid,
                     COF: COF,
                     order_code: order_code,
                     job_id: job_id_22,
                     department: department,
                     season: season,
                     exclusive_trims: exclusive_trims,
                     fiber_1: fiber_1,
                     fiber_per_1: fiber_per_1,
                     dialog4: dialog4,
                     dialog5: dialog5,
                     dialog6: dialog6,
                     dialog7: dialog7,
                     dialog8: dialog8,
                     dialog9: dialog9,
                     customer_address: customer_address,
                     delivery_address: delivery_address,
                     contact_person_customer: contact_person_customer,
                     contact_person_delivery: contact_person_delivery,
                     customer_contact_person: customer_contact_person,
                     delivery_contact_person: delivery_contact_person
                 },

            ];
            /////////////////Data for Per set Two End////////////////////////
            var data = "";
            if ($("#2_partset").is(':checked')) {
                data = [{ "ComboData": comboData, "AllData": allData }, { "ComboData": comboData_2, "AllData": allData_2 }];
            } else if ($("#3_partset").is(':checked')) {
                data = [{ "ComboData": comboData, "AllData": allData }, { "ComboData": comboData_2, "AllData": allData_2 }, { "ComboData": comboData_3, "AllData": allData_3 }];
            } else if ($("#4_partset").is(':checked')) {
                data = [{ "ComboData": comboData, "AllData": allData }, { "ComboData": comboData_2, "AllData": allData_2 }, { "ComboData": comboData_3, "AllData": allData_3 }, { "ComboData": comboData_4, "AllData": allData_4 }];
            } else {
                data = [{ "ComboData": comboData, "AllData": allData }];
            }

            //var data = { "ComboData": comboData, "AllData": allData };
            $.ajax({
                type: "POST",
                url: "@UrlConfig.Action("GetOrder", "SaveOrder")",
                dataType: 'json',
                contentType: "application/json",
                data: JSON.stringify(data),
                success: function (data) {
                    if (data.output === "error") {
                        var message = '<div class="col-md-12" style="border: 3px solid green; height: 35px; color: red; padding-top:4px; font-size:15px;">' + data.msg + '</div><div class="clearfix"></div><br/>';
                    } else {

                        var job_id = data[0].job_id;
                        if ($("#is_bagadh").is(':checked')) {
                            if ($("#is_BEIP").is(':checked')) {
                                var url = '@Url.Action("TuBagadh", "OrderEntryMainTU")?job_id=' + job_id + "&beip=1";
                                window.location.href = url;
                            } else {
                                var url = '@Url.Action("TuBagadh", "OrderEntryMainTU")?job_id=' + job_id + "&customer_id=" + customer_id;
                                window.location.href = url;
                            }
                        }
                        else {
                            var message = '<div class="col-md-12" style="border: 3px solid green; height: 35px; color: red; padding-top:4px; font-size:15px;">Your order has been succesfully saved. Your order traking number is ' + job_id + '</div><div class="clearfix"></div><br/>';
                            $("#grid").hide(500);
                            $('#grid_3').hide(500);
                            $('#grid_2').hide(500);
                            $('#grid_4').hide(500);
                            $('#common-Field').hide(500);
                            $('#common-Field_2').hide(500);
                            $('#common-Field_3').hide(500);
                            $('#common-Field_4').hide(500);
                            $("#btnSubmitOrder").hide(500);
                        }

                    }
                    $("#msg_container_1").html(message);

                }
            });

        });
    });

</script>

<script type="text/javascript">
    $(document).ready(function () {
        $('#grid_3').hide();
        $('#grid_2').hide();
        $('#grid_4').hide();
        $('#common-Field_2').hide();
        $('#common-Field_3').hide();
        $('#common-Field_4').hide();

        $('#btnSubmitOrder_2').hide();
        $('#btnSubmitOrder_3').hide();
        $('#btnSubmitOrder_4').hide();


        $('#2_partset').change(function () {
            if (this.checked) {
                $('#grid_2').show();
                $('#btnSubmitOrder_2').show();
                $('#common-Field_2').show();
                $('#common-Field_3').hide();
                $('#common-Field_4').hide();
                $('#grid_3').hide();
                $('#grid_4').hide();
                $('#btnSubmitOrder_3').hide();
                $('#btnSubmitOrder_4').hide();

                $('#3_partset').not(this).prop('checked', false);
                $('#4_partset').not(this).prop('checked', false);

            } else {
                $('#grid_2').hide();

                $('#common-Field_2').hide();

                $('#btnSubmitOrder_2').hide();
            }
        });

        $('#3_partset').change(function () {
            if (this.checked) {
                $('#grid_3').show();
                $('#grid_2').show();
                $('#grid_4').hide();

                $('#common-Field_2').show();
                $('#common-Field_3').show();
                $('#common-Field_4').hide();

                $('#btnSubmitOrder_2').show();
                $('#btnSubmitOrder_3').show();
                $('#btnSubmitOrder_4').hide();

                $('#2_partset').not(this).prop('checked', false);
                $('#4_partset').not(this).prop('checked', false);

            } else {
                $('#grid_2').hide();
                $('#grid_3').hide();

                $('#common-Field_2').hide();
                $('#common-Field_3').hide();

                $('#btnSubmitOrder_2').hide();
                $('#btnSubmitOrder_3').hide();
            }
        });
        $('#4_partset').change(function () {
            if (this.checked) {
                $('#grid_3').show();
                $('#grid_2').show();
                $('#grid_4').show();

                $('#common-Field_2').show();
                $('#common-Field_3').show();
                $('#common-Field_4').show();

                $('#btnSubmitOrder_2').show();
                $('#btnSubmitOrder_3').show();
                $('#btnSubmitOrder_4').show();

                $('#3_partset').not(this).prop('checked', false);
                $('#2_partset').not(this).prop('checked', false);

            } else {
                $('#grid_3').hide();
                $('#grid_2').hide();
                $('#grid_4').hide();

                $('#common-Field_2').hide();
                $('#common-Field_3').hide();
                $('#common-Field_4').hide();

                $('#btnSubmitOrder_2').hide();
                $('#btnSubmitOrder_3').hide();
                $('#btnSubmitOrder_4').hide();
            }
        });
    });
</script>
