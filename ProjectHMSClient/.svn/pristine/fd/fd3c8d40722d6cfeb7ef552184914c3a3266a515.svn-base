                //serverPaging: true,
                //serverFiltering: true,
                //serverSorting: true
            });
            jQuery("#grid").kendoGrid({
                dataSource: dataSource,
                filterable: {
                    extra: false,
                    operators: {
                        string: {
                            startswith: "Starts with",
